{
  "name": "poste",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "yarn run clean && react-native run-android --variant debug && yarn run sentry-release",
    "android-release": "yarn run clean && react-native run-android --variant release && yarn run sentry-release",
    "ios": "react-native run-ios && yarn run sentry-release",
    "apk": "cd android && ./gradlew clean && ./gradlew assembleRelease  && yarn run sentry-release",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "pod-install": "cd ios && pod install",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "cd android && ./gradlew clean && cd ..",
    "sentry-release": "cross-var sentry-cli releases --org proxym-la --project poste files $npm_package_version upload-sourcemaps --url-prefix '~/static/js' --validate build/static/js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-community/netinfo": "^9.3.7",
    "@react-navigation/native": "^6.1.3",
    "@reduxjs/toolkit": "^1.9.2",
    "@sentry/react-native": "^4.14.0",
    "axios": "^1.3.2",
    "cross-var": "^1.1.0",
    "i18next": "^22.4.9",
    "jwt-check-expiration": "^1.0.5",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-i18next": "^12.1.4",
    "react-native": "0.71.1",
    "react-native-config": "^1.5.0",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0",
    "redux-subscriber": "^1.1.0",
    "redux-undo": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.24",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-subscriber": "^1.1.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-native-dotenv": "^3.4.7",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
